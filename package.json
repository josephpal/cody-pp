{
  "name": "cody-pp",
  "description": "A simple and fast web IDE to create C++ applications for ESP32 microcontrollers based on arduino-code syntax",
  "version": "1.0.0",
  "author": "Joe",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_APP_MODE=app webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production NODE_APP_MODE=app webpack --progress --hide-modules",
    "demo": "cross-env NODE_ENV=production MODE=demo NODE_APP_MODE=app webpack --progress --hide-modules",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "svg": "node ./build/svg.js"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "node-blockly": "^1.0.31",
    "vue": "2.5.21",
    "vue-js-toggle-button": "^1.3.2",
    "vue-router": "3.0.2",
    "vuebar": "0.0.17"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-preset-env": "^1.6.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^1.1.6",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sinon-chai": "^1.3.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "lolex": "^1.5.2",
    "mocha": "^3.2.0",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "sass-loader": "^6.0.6",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.18.2",
    "svgstore": "^3.0.0-2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "2.5.21",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
