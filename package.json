{
  "name": "cody-pp",
  "description": "A simple and fast web IDE to create C++ applications for ESP32 microcontrollers based on arduino-code syntax",
  "version": "1.0.0",
  "author": "Joe",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_APP_MODE=app webpack serve",
